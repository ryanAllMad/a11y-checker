{
	"compilerOptions": {
		"baseUrl": "./src",
		"target": "es6",
		"lib": ["dom", "dom.iterable", "es6"],
		"outDir": "./dist/js",
		"noImplicitAny": true,
		"jsx": "react",
		"allowJs": true,
		"moduleResolution": "node",
    "allowSyntheticDefaultImports": true
},
	"include": ["src"],
	"exclude": ["node_modules"]
}
